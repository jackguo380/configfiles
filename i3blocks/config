[Sound]
command=$HOME/.config/i3blocks/soundoutput.sh
interval=once

[Volume]
command=$HOME/.config/i3blocks/soundvolume.sh
interval=5
signal=10

[IP]
command=echo IP: $(hostname -I)
interval=30

[Shutdown]
command=cinnamon-session-quit --power-off
full_text=Shutdown

[Time]
command=date "+%Y-%m-%d %H:%M:%S"
interval=5
